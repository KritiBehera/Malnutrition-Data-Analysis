# Using matplotlib.pyplot for creating Bars:
import matplotlib.pyplot as
pltimport numpy as np
x = np.array(["A", "B", "C", "D"])
y = np. array ([3, 8, 1,
10])plt.bar(x,y)
plt.show()

# Using matplotlib.pyplot for creating Pie chart:
import matplotlb.pyplot asplt
import NumPy as np
y = np.array([35, 25, 25, 15])
mylabels = ["Apples", "Bananas", "Cherries”, “Dates"]
plt.pie(y)
plt.show()

# Using matplotlib.pyplot for creating Histogram:
import matplotlib.pyplot as plt
import NumPy as np
x=np.random.normal(170,10,250);
plt.hist(x);
plt.show();

# Use of seaborn
import matplotlib.pyplot as plt
import seaborn as sns
sns.distplot([0,1,2,3,4,5])
plt.show()
